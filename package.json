{
  "name": "aluxion_backend_test",
  "version": "1.0.0",
  "description": "Proyecto MVC, Nodejs, Bienes Raices",
  "main": "dist/app.js",
  "scripts": {
    "test": "mocha --require ts-node/register src/**/*.test.ts src/**/*.test.ts",
    "test:watch": "mocha --require ts-node/register --watch --watch-files domains/**/*.ts,test/**/*.test.ts,src/**/*.test.ts test/**/*.test.ts src/**/*.test.ts",
    "build": "tsc",
    "prod": "npm run clean && tsc && npm run copyFolder && npm run triggerDist",
    "copyFolder": "copyfiles --u 1 ./src/frontend/** ./dist",
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "triggerDist": "node ./dist/app.js",
    "start": "npm run prod",
    "docker": "npm i  && npm run start",
    "clean": "rimraf dist"
  },
  "keywords": [
    "MCV",
    "Pug",
    "Tailwind",
    "MySQL",
    "Sequelize"
  ],
  "author": "Carlos Silvestri @carlossilvestri @Silvestri_C",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1166.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.14.2",
    "http-status": "^1.5.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.5",
    "pug": "^3.0.2",
    "sequelize": "^6.21.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/http-status": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/sequelize": "^4.28.13",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "autoprefixer": "^10.4.7",
    "chai": "^4.3.6",
    "copy-dir": "^1.3.0",
    "copyfiles": "^2.4.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^1.1.8"
  }
}
